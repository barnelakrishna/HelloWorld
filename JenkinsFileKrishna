pipeline{
    agent any
    tools{
        maven 'maven3.9.0'
    }
    
    stages{
        stage('CheckOutCode'){
            steps{
                git branch: 'devlopment', credentialsId: '40de6d70-6950-4e26-a985-2a8e13c20a40', url: 'https://github.com/barnelakrishna/HelloWorld.git'
            }
        }
        
        stage('Build'){
            steps{
                sh "mvn clean package"
            }
        }
    }
}
